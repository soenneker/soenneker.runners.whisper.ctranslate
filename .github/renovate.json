{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "automerge": true,
  "prHourlyLimit": 0,
  "autoApprove": true,
  "packageRules": [
    {
      "matchDepNames": [ "actions/setup-python" ],
      "enabled": false
    },
    {
      "matchManagers": [ "github-actions" ],
      "matchDepNames": [ "python" ],
      "enabled": false
    }
  ]
}